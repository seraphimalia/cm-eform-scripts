  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.5",
    "babel-preset-minify": "0.5.1",
    "eslint": "7.12.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "^16.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "http-server": "14.0.0",
